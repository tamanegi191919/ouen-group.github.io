    <pattern id="community-bg-pattern" patternUnits="userSpaceOnUse" width="200" height="200">
      <rect width="200" height="200" fill="#f8f9fa"/>
      
      <!-- 歯のアイコン -->
      <path d="M40,40 C40,30 45,25 50,25 C55,25 60,30 60,40 C60,50 55,60 50,60 C45,60 40,50 40,40 Z" fill="#e6f7ff" stroke="#0088cc" stroke-width="1" opacity="0.3"/>
      
      <!-- 家のアイコン -->
      <path d="M140,40 L150,30 L160,40 L160,55 L130,55 L130,40 L140,30 Z" fill="#e6f7ff" stroke="#0088cc" stroke-width="1" opacity="0.3"/>
      
      <!-- 人のアイコン -->
      <circle cx="50" cy="120" r="5" fill="#0088cc" opacity="0.2"/>
      <path d="M50,125 L50,140 M45,130 L55,130 M50,140 L45,150 M50,140 L55,150" stroke="#0088cc" stroke-width="1.5" opacity="0.2"/>
      
      <!-- 病院のアイコン -->
      <rect x="130" y="110" width="30" height="20" fill="#e6f7ff" stroke="#0088cc" stroke-width="1" opacity="0.3"/>
      <rect x="135" y="130" width="20" height="20" fill="#e6f7ff" stroke="#0088cc" stroke-width="1" opacity="0.3"/>
      <path d="M145,115 L145,125 M140,120 L150,120" stroke="#0088cc" stroke-width="1.5" opacity="0.3"/>
    </pattern>
  </defs>
  
  <rect width="1200" height="800" fill="url(#community-bg-pattern)"/>
  
  <!-- グラデーションオーバーレイ -->
  <rect width="1200" height="800" fill="white" opacity="0.7"/>
</svg> 